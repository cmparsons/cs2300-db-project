{
  "name": "cs2300-project-server",
  "version": "1.0.0",
  "description": "Databases project for CS2300",
  "main": "index.js",
  "scripts": {
    "install-client": "cd ../client && npm install",
    "install-all": "npm install && npm run install-client",
